<script type="text/javascript">
var AOLSEARCH = AOLSEARCH || {};
AOLSEARCH.searchBox = (function () {
/****** UTIL FUNCTIONS START ******/
var attachEvent = function (el, m, f){
if (el.addEventListener) {
el.addEventListener(m, f, false);
}
else {
if (el.attachEvent) {
el.attachEvent("on" + m, f);
}
}
};
var detachEvent = function(el, m, f){
if (el.removeEventListener) {
el.removeEventListener(m, f, false);
}
else {
if (el.detachEvent) {
el.detachEvent("on" + m, f);
}
}
};
var addLoadEvent = function(func){
var oldonload = window.onload;
if (typeof window.onload != 'function'){
window.onload = func;
} else {
window.onload = function(){
oldonload();
func();
}
}
};
//preventing from safari cache on landing page
window.onunload=function(){};
/****** UTIL FUNCTIONS END ******/

var getWidth = function(el)
{
var width = 0;
if(el){
width = el.clientWidth;
}
if(parseInt(width) == 0 && el && !window.getComputedStyle){
width = getComputedStyle( el ).width;
}
return parseInt(width);
}
var setSearchBoxWidth = function()
{
var sBoxBtn = document.getElementById('buttonContainer');
var sBoxInput = document.getElementById("sboxQuery");
var sBoxContainer = document.getElementById("inner");
var totalWidth = getWidth(sBoxBtn) + getWidth(sBoxInput) + 6;
sBoxContainer.style.width = totalWidth + "px";
}
setSearchBoxWidth();

var loadAutoCompleteJSON = function()
{

var AOLSEARCH = AOLSEARCH || {};
if(typeof(autoSuggest)==="undefined"){
var e=document.createElement("script");
var _2=asseturi+"autocomplete.js";
e.src=_2;
e.type="text/javascript";
document.getElementsByTagName("head")[0].appendChild(e);
detachEvent(document.getElementById("sboxQuery"), "focus", loadAutoCompleteJSON);
}

};
var setAutoCompleteLoadEvent = function (){
if (document.getElementById("sboxQuery")) {
sboxfrm = document.getElementById("sboxQuery");
attachEvent(sboxfrm, "focus", loadAutoCompleteJSON);
}
};
setAutoCompleteLoadEvent();


var cursorAtBeginning = function (searchBox){
if (searchBox.setSelectionRange) {
searchBox.setSelectionRange(0, 0);
} else { //IE
if (searchBox.createTextRange) {
var range = searchBox.createTextRange();
range.collapse(true);
range.moveEnd('character', 0);
range.moveStart('character', 0);
range.select();
}
}
};
var focusSearchBox = function (){
var searchBox=document.getElementById("sboxQuery");
if(null != searchBox) {
searchBox.focus();
cursorAtBeginning(searchBox);
}
};
addLoadEvent(function (){
focusSearchBox();
});


var sBoxSubmitAction = function () {
var searchBox = document.getElementById('sboxQuery');
if (searchBox) {
if (searchBox.className == "sbox ghost"
&& searchBox.value == "")
{
searchBox.value = "";
}
}
document.getElementById('sboxform').submit();
};

/**** Public Function To expose JS functions ****/
publicf = {
init:function (func){
func();
},
searchBoxclearText: function (node) {
searchBoxclearText(node);
},
sBoxSubmitAction:sBoxSubmitAction,
autoinitcb:autoinitcb
};

return publicf;
}());

var autoinitcb = function (){
AOLSEARCH.autoSuggest.init('sboxQuery','sboxform','sboxform','http://autocomplete.search.aol.com/autocomplete/get?q=AC_QUERY&it=search-thp-ac&locale=en_US,en_*,*_US,*&count=4&output=json&cb=AOLSEARCH.autoSuggest.display&echo=off','en_US_search','/search?');
};
</script>
