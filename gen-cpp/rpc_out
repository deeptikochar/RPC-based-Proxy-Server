<script type="text/javascript" src= "/static/global/scripts/thirdparty/mtagconfig.js"></script>
<script type="text/javascript">
var LivePerson = THD.Utility.Namespace.createNamespace("LivePerson", THD);
LivePerson.load = function() {
  try{
    if (typeof(lpUnit)=='undefined') var lpUnit='global';
    if(typeof(lpAddVars)!="undefined") lpAddVars('page','unit',lpUnit);

    if (typeof(lpLanguage)=='undefined') var lpLanguage='english';
    if(typeof(lpAddVars)!="undefined") lpAddVars('session','language',lpLanguage);

    lpMTagConfig.defaultInvite = "chat-" + lpUnit + "-" + lpLanguage;
  } catch(e){}
 lpAddVars('page','PageId',"");
 lpAddVars('page','Hierarchy',"");
 var isLoggedOn = 'no';
 if(readCookie("THD_USERSTATUS") == '1') isLoggedOn = 'yes';
 lpAddVars('session','RegisteredUser',isLoggedOn);
  if (typeof(lpMTagConfig.dynButton) !== "undefined") {
    var $lpButtons = $(".lpbutton");
    if ($lpButtons.length > 0) {
      $lpButtons.each(function() {
        var $this = $(this);
        var dynButton = {};
        var lpName = $this.attr("data-name");
        if (lpName) {
          dynButton.name = lpName;
        }
        var lpPid = $this.attr("data-pid");
        if (lpPid) {
          dynButton.pid = lpPid;
        }
        var lpAfterStartPage = $this.attr("data-after-start-page");
        if (lpAfterStartPage) {
          dynButton.afterStartPage = lpAfterStartPage;
        }
        var lpOvr = $this.attr("data-ovr");
        if (lpOvr) {
          dynButton.ovr = lpOvr;
        }
        lpMTagConfig.dynButton[lpMTagConfig.dynButton.length] = dynButton;
      });
    }
  }
};
 $(document).ready(function() {
    THD.LivePerson.load();
      });
</script>
