<script>
function chkform(formName)
{
	
	
	var key = $('#'+formName).find('input[name=q]').val();
	var flag = 'w';
	var action = "/search/result/2/";
	var fr = $("#search_type").val();
	
	if(key == '' || key == null)
	{
		key = $(".m_search_kw").val();
		flag = 'm';
	}

	if(key != '')
	{
		if(flag == 'm')	fr = $("#m_search_type").val();
		key = key.replace('%', ' ');
		action += key;
		/*
		if(fr == "websearch")
			action = "http://udn.com/search/";
		else if(fr == "udndata")
			action = "http://data.udn.com/data/Search";
		*/
		window.location.href = action;
	}
	else
		return false;
}
</script>
 
