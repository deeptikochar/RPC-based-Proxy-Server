	 desc = 'A-Lin雖敗猶榮 給自己100分 ';     listDiv.push('<dt><a href="'+url+'"><img src="'+img+'" width="200" /><h4>'+desc+'</h4></a></dt>');
	url = 'http://udn.com/news/story/7001/802834';
	img = 'http://pgw.udn.com.tw/gw/photo.php?u=http://uc.udn.com.tw/photo/2015/03/29/1/666105.jpg&sl=W&fw=200';
	 desc = '中職／路易士、帝波開球 打擊玩真的';     listDiv.push('<dt><a href="'+url+'"><img src="'+img+'" width="200" /><h4>'+desc+'</h4></a></dt>');

$(window).scroll(function() {
	if($(window).scrollTop() + $(window).height() == $(document).height()) {
		add_list(reset_list);
	}
});

function add_list(callback){
	img_list_start += img_list_limit;
	if(img_list_start < total_imgs_of_today) {
		var listShow = listDiv.slice(img_list_start, img_list_start+img_list_limit);
		$('#loadmore').attr('style', 'display:block;');
		setTimeout(function() {
			for(var i = 0; i < img_list_limit; i++) {
				$('#photo_dtst').append(listShow[i]);
			}
		}, 50*img_list_limit);	
		setTimeout(function() {
			$('#loadmore').attr('style', 'display:none');
			callback();
		}, 100*img_list_limit);
	}
}

function reset_list(){
  var handler = $('#photo_dtst dt a');
  handler.wookmark({
	// Prepare layout options.
	// autoResize: true, // This will auto-update the layout when the browser window is resized.
	container: $('#photo_body'), // Optional, used for some extra CSS stydtng
	offset: 8, // Optional, the distance between grid items
	outerOffset: 16
  });
}
</script>
				
