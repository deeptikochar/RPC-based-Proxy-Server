<script>
showmore = function(channel_id,cate_id,sub_id) {

	if (sub_id != '') {
		var moreSub = $("#showmore_"+sub_id);
		var moreId = $("#moreId_"+sub_id).attr("value");
		moreId = parseInt(moreId)+parseInt(1);
		var url = '/news/get_article/'+moreId+'/'+channel_id+'/'+cate_id+'/'+sub_id;
	}
	else {
		var moreSub = $("#showmore_"+cate_id);
		var moreId = $("#moreId_"+cate_id).attr("value");
		moreId = parseInt(moreId)+parseInt(1);
		var url = '/news/get_article/'+moreId+'/'+channel_id+'/'+cate_id;
	}

	var subDiv = moreSub.parent().find("div.category_box_list > dl");
	var subDivBody = subDiv.html();

	$.ajax({
		url: url,
		cache: false,
		success: function(response) {
			var sub_res = response.substr(0,8);
			subDiv.html(subDivBody+response);
			$("#moreId_"+sub_id).attr("value",moreId);

			if (sub_res == '<!--N-->') moreSub.css('display','none');
		}
	});
}


$(function(){

	$('#channel_tab a').mouseenter(function(){
		var channel_tab = $(this);
		trigger_channel_tab = setTimeout(function() {
			$("#content").load( channel_tab.attr('rel') + '?' + (Math.random()*99999).toString() );
			$("#channel_tab a").removeClass('active');
			channel_tab.addClass('active');
		}, 400);
	});

	$('#channel_tab a').mouseleave(function(){
		clearTimeout(trigger_channel_tab); // 清除上面的延迟触发的事件
	})

});

$(window).on("load scroll resize",function(e) {
	if ( $("#ad_content").length > 0 && $(window).width() <= 1016 && $(window).width() >= 750 ) { 
		$("#sidebar").css('margin', -($("#mainbar").height()-$("#content").height()-$("#ad_content").height()-22)+ 'px 0 0');
		$("#ad_content").css('margin', '0 0 ' + ($("#sidebar").height()+60)+ 'px' );
	}
});

</script>





