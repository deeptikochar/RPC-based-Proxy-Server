        <script language="JavaScript"> 
        <!--

        /* 左邊補0 */
        function padLeft(str, len) {
          str = '' + str;
          return str.length >= len ? str : new Array(len - str.length + 1).join("0") + str;
        }

        function calendar()
        {
        var today,year,month,thisday,nDays;
        var yest_year,yest_month,yestday,yest_nDays;
        today = new Date();
        year = today.getFullYear() + 1900;
        yest_year = today.getFullYear();
        this_year = today.getFullYear();
        month = today.getMonth();
        yest_month = today.getMonth();
        if(yest_month == 0)
        {
            yest_month = 12;
            yest_year = (today.getFullYear()-1);
        }
        this_month = (today.getMonth()+1);
        thisday = today.getDate()-1;
        totalshow = 30;
        yestday = totalshow - thisday;
        if(yestday < 0)
        {
            yestday = 0;
        }
        if ((yest_month-1) == 1)
        {
            if ( ((yest_year % 4 == 0) && (yest_year % 100 != 0)) || (yest_year % 400 == 0) )
            {
                yest_nDays = 29-yestday;
            }
            else
            {
                yest_nDays = 28-yestday;
            }
        }
        else if (((yest_month-1) == 3)||((yest_month-1) == 5)||((yest_month-1) == 8)||((yest_month-1) == 10))
        {
            yest_nDays = 30-yestday;
        }
        else
        {
            yest_nDays = 31-yestday;
        }
         
        if (month == 1) {
        if ( ((year % 4 == 0) && (year % 100 != 0)) || (year % 400 == 0) ){
        nDays = 29;
        } else {
        nDays = 28;
        }
        } else if ((month == 3)||(month == 5)||(month == 8)||(month == 10)){
        nDays = 30;
        } else {
        nDays = 31;
        }
         
        firstDay = today;
        firstDay.setDate(1-(yestday%7));
        startDay = firstDay.getDay();
        document.write('<table border="0" align="center" cellpadding="0" cellspacing="0">');
        document.write('<tr valign="bottom">');
        document.write('<th class="updown" height="20">日</th>');
        document.write("<th>一</th>");
        document.write("<th>二</th>");
        document.write("<th>三</th>");
        document.write("<th>四</th>");
        document.write("<th>五</th>");
        document.write('<th class="updown">六</th>');
        document.write("</tr>");
         
         
         
         
        document.write("<tr>");
        column = 0;
        for (i=0; i<startDay; i++) {
        document.write("<td></td>");
        column++;
        }
        for (i=1; i<=yestday; i++) {
        yest_day = (i+yest_nDays);
        if(yest_day > 0)
        {
        document.write("<td>");
        document.write("<a href=/news/archive/" + yest_year + "/" + padLeft(yest_month,2) + "/" + padLeft(i+yest_nDays,2) + " target=\"_top\" title=\""+ yest_year + "/" + yest_month + "/" + (i+yest_nDays) +"\" class=\"yest\">");
        document.write(i+yest_nDays);
        document.write("</a>");
        document.write("</td>");
        column++;
        }
        if (column == 7) {
        document.write("<tr>");
        column = 0;
        }
        }
         
         
         
         
        for (i=1; i<=thisday; i++)
        {
            document.write("<td>");
            if (i <= thisday) document.write("<a href=/news/archive/" + this_year + "/" + padLeft(this_month,2) + "/" + padLeft(i,2) + " target=\"_top\" title=\""+ this_year + "/" + this_month + "/" + i +"\">");
            if (i == thisday) document.write("<b>");
            document.write(i);
            if (i == thisday) document.write("</b>");
            if (i <= thisday) document.write("</a>");
            
            document.write("</td>");
            column++;
            
            if (column == 7) {
            document.write("<tr>");
            column = 0;
            }
        }
         
        document.write("</table>");
        }
